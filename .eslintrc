{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent":"off",
        "linebreak-style":"off",
        "quotes":"off",
        "require-jsdoc":"off",
        "operator-linebreak":"off",
        "object-curly-spacing":"off"
    },
    "extends": ["eslint:recommended", "google"]
}